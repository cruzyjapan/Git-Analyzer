{
  "name": "git-analyzer",
  "version": "3.0.0",
  "description": "Advanced Git repository analyzer with file content analysis, complete diff display, and AI CLI integration",
  "main": "src/index.js",
  "type": "module",
  "bin": {
    "git-analyzer": "./src/index.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js"
  },
  "keywords": [
    "git",
    "analyzer",
    "diff",
    "ai",
    "cli",
    "development-tools"
  ],
  "author": "Development Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "inquirer": "^9.2.12",
    "joi": "^17.11.0",
    "lodash": "^4.17.21",
    "marked": "^11.1.0",
    "moment": "^2.29.4",
    "ora": "^7.0.1",
    "simple-git": "^3.20.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/git-analyzer.git"
  }
}
